        </activity>
        <activity
            android:name=".ListaNotasActivity"
            android:label="@string/ListaNotasActivityNAME" />
        <activity
            android:name=".NoteActivity"
            android:label="@string/NoteActivityNAME" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/LoginActivityNAME" />
        <activity
            android:name=".PrivateActivity"
            android:label="@string/NotasPrivadasActivityNAME" />
        <activity android:name=".PrivNoteActivity"
            android:label="@string/PrivNoteActivityNAME" ></activity>

        <activity android:name=".FingerActivity" />
